{
  "name": "frontend",
  "version": "1.0.0",
  "private": true,
  "main": "next.config.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "vite-build": "vite build",
    "preview": "vite preview",
    "generate": "prisma generate",
    "export-dev": "tsx --loader ./ignore-css-loader.js devServer.ts",
    "codemod": "tsc && node dist/fix-import-extensions.mjs --dry",
    "build": "tsc -p tsconfig.build.json && vite build",
    "build-dev": "tsc -p tsconfig.dev.json && vite build",
    "build-prod": "tsc -p tsconfig.build.json && vite build",
    "build-all": "BUILD_TELEMETRY=true npm run build && npm run build",
    "build-devServer": "tsc -p tsconfig.devServer.json",
    "start-dev": "NODE_ENV=development tsx devServer.mts",
    "start-prod": "NODE_ENV=production node build/start-server.js",
    "start": "node devServer.mjs",
    "postbuild": "rsync -a build/src/ deploy/src/",
    "setup-venv": "python3 -m venv .venv && ./.venv/bin/python3 -m pip install pdf2docx PyMuPDF && echo 'VIRTUAL_ENV=./.venv' > .env.local"
  },
  "keywords": [],
